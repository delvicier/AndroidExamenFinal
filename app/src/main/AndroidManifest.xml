<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.conoce_quito">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Conoce_Quito">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="${MAPS_API_KEY}" />

        <activity
            android:name=".Carolina"
            android:exported="false"
            android:label="@string/title_activity_maps4" />
        <activity
            android:name=".Basilica"
            android:exported="false"
            android:label="@string/title_activity_maps3" />
        <activity
            android:name=".Templo"
            android:exported="false"
            android:label="@string/title_activity_maps2" />
        <activity
            android:name=".Museo"
            android:exported="false"
            android:label="@string/title_activity_maps" />
        <activity
            android:name=".Ubicacion4"
            android:exported="false" />
        <activity
            android:name=".Ubicacion3"
            android:exported="false" />
        <activity
            android:name=".Ubicacion2"
            android:exported="false" />
        <activity
            android:name=".Ubicacion1"
            android:exported="false" />
        <activity
            android:name=".Menu"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>